{
  "name": "@ugrc/parcels",
  "version": "2.3.18",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "pnpm start",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "start:ci": "vite --host",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm run lint"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/esri-theme-toggle": "^1.1.2",
    "@ugrc/tsconfigs": "^1.0.3",
    "@ugrc/utah-design-system": "^2.0.0",
    "@ugrc/utilities": "^3.1.1",
    "firebase": "^12.2.1",
    "ky": "^1.14.3",
    "lodash.debounce": "^4.0.8",
    "lodash.startcase": "^4.4.0",
    "react": "^19.2.4",
    "react-aria": "^3.43.2",
    "react-aria-components": "^1.12.2",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-fast-compare": "^3.2.2",
    "react-stately": "^3.41.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.24",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "firebase-tools": "^15.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "esbuild",
      "protobufjs",
      "re2"
    ],
    "overrides": {
      "tmp@<=0.2.3": ">=0.2.4",
      "vite@>=6.0.0 <=6.3.5": ">=6.3.6",
      "tar@=7.5.1": ">=7.5.2",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "jws@=4.0.0": ">=4.0.1",
      "jws@<3.2.3": ">=3.2.3",
      "storybook@>=9.0.0 <9.1.17": ">=9.1.17",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<=7.5.3": ">=7.5.4",
      "lodash-es@>=4.0.0 <=4.17.22": ">=4.17.23",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "tar@<7.5.7": ">=7.5.7",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "qs@<6.14.1": ">=6.14.1"
    },
    "ignoredBuiltDependencies": [
      "@vaadin/vaadin-usage-statistics"
    ]
  }
}
