{
  "name": "@ugrc/parcels",
  "version": "2.3.15",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "pnpm start",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "start:ci": "vite --host",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm run lint"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.33.10",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/esri-theme-toggle": "^1.1.1",
    "@ugrc/layer-selector": "^6.4.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@ugrc/utah-design-system": "^1.27.3",
    "@ugrc/utilities": "^3.0.4",
    "firebase": "^11.10.0",
    "ky": "^1.8.2",
    "lodash.debounce": "^4.0.8",
    "lodash.startcase": "^4.4.0",
    "react": "^19.1.0",
    "react-aria": "^3.41.1",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-fast-compare": "^3.2.2",
    "react-stately": "^3.39.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.31.0",
    "firebase-tools": "^14.10.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "esbuild",
      "protobufjs",
      "re2"
    ],
    "overrides": {},
    "ignoredBuiltDependencies": [
      "@vaadin/vaadin-usage-statistics"
    ]
  }
}
