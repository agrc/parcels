{
  "name": "parcels",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "ky": "^0.28.6",
    "lodash.debounce": "^4.0.8",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-toastify": "^8.0.3"
  },
  "devDependencies": {
    "@arcgis/core": "^4.21.2",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^27.3.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.3.1",
    "mocha": "^9.1.3",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.43.3",
    "tailwindcss": "^2.2.17",
    "vite": "^2.6.11",
    "vite-plugin-react": "^4.0.1",
    "vite-react-jsx": "^1.1.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "prettier/prettier": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  }
}
